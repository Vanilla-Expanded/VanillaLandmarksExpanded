			<graphicClass>Graphic_Random</graphicClass>
			<shaderType>Transparent</shaderType>
			<texPath>Things/Building/Sinkhole</texPath>
			<drawSize>(9, 9)</drawSize>
		</graphicData>
		<size>(9, 9)</size>
		<building>
			<uninstallWork>15000</uninstallWork>
			<deconstructible>false</deconstructible>
			<isTargetable>false</isTargetable>
			<isInert>true</isInert>
			<claimable>false</claimable>
			<expandHomeArea>false</expandHomeArea>
			<crater>true</crater>
			<canLandGravshipOn>true</canLandGravshipOn>
		</building>
		<statBases>
			<Flammability>0</Flammability>
			<Beauty>-20</Beauty>
		</statBases>
		<comps>
			<li Class="CompProperties_LeaveFilthOnDestroyed">
				<filthDef>Filth_LooseGround</filthDef>
				<thickness>5</thickness>
			</li>
		</comps>
	</ThingDef>
