	<ThingDef ParentName="BuildingNaturalBase">
		<defName>VEE_Coral</defName>
		<label>coral</label>
		<description>A colorful, calcified colony of tiny marine organisms, forming intricate reef structures teeming with life.</description>
		<thingClass>VanillaExplorationExpanded.Building_FakePlant2x2</thingClass>
		<graphicData>
			<texPath>Things/Plant/Coral</texPath>
			<graphicClass>Graphic_Random</graphicClass>
			<drawSize>0.75</drawSize>
		</graphicData>
		<uiIconScale>0.5</uiIconScale>
		<altitudeLayer>Floor</altitudeLayer>
		<rotatable>false</rotatable>
		<destroyable>false</destroyable>
		<useHitPoints>false</useHitPoints>
		<selectable>true</selectable>
		<pathCost>100</pathCost>
		<size>(2,2)</size>
		<neverOverlapFloors>true</neverOverlapFloors>
		<building>
			<isEdifice>false</isEdifice>
			<claimable>false</claimable>
			<deconstructible>false</deconstructible>
		</building>
	</ThingDef>
	<ThingDef ParentName="BuildingNaturalBase">
		<defName>VEE_SulfurVent</defName>
		<label>sulfur fissure</label>
		<description>A naturally-occurring break in the ground from which sulfuric gases escape. The gas produced on these fissures stinks of rotten eggs.</description>
		<thingClass>Building</thingClass>
		<graphicData>
			<texPath>Things/Building/GeothermalVent</texPath>
			<graphicClass>Graphic_Single</graphicClass>
			<drawSize>(3,3)</drawSize>
		</graphicData>
		<comps>
			<li Class="CompProperties_FleckEmitterLongTerm">
				<fleckDef>VEE_SulfurCloud</fleckDef>
				<spawnRadius>0.2</spawnRadius>
				<spawnChance>0.05</spawnChance>
				<spawnOffsetFromCenter>(0.5,0.0,0.5)</spawnOffsetFromCenter>
				<prewarmCycles>1.0</prewarmCycles>
				<fleckScale>2.0</fleckScale>
				<minRotationRate>-10.0</minRotationRate>
				<maxRotationRate>10.0</maxRotationRate>
				<minVelocityAngle>-20.0</minVelocityAngle>
				<maxVelocityAngle>20.0</maxVelocityAngle>
				<minVelocitySpeed>0.5</minVelocitySpeed>
				<maxVelocitySpeed>0.7</maxVelocitySpeed>
				<forceEnabled>true</forceEnabled>
			</li>
		</comps>
		<uiIconScale>0.5</uiIconScale>
		<altitudeLayer>Floor</altitudeLayer>
		<rotatable>false</rotatable>
		<destroyable>false</destroyable>
		<useHitPoints>false</useHitPoints>
		<tickerType>Normal</tickerType>
		<selectable>true</selectable>
		<size>(3,3)</size>
		<neverOverlapFloors>true</neverOverlapFloors>
		<blockPlants>true</blockPlants>
		<building>
			<isEdifice>true</isEdifice>
			<canBuildNonEdificesUnder>false</canBuildNonEdificesUnder>
			<deconstructible>false</deconstructible>
			<repairable>false</repairable>
			<isTargetable>false</isTargetable>
			<isInert>true</isInert>
			<claimable>false</claimable>
			<expandHomeArea>false</expandHomeArea>
		</building>
	</ThingDef>
	<ThingDef ParentName="BuildingNaturalBase">
		<defName>VEE_DryWheat</defName>
		<label>dry wheat</label>
		<thingClass>VanillaExplorationExpanded.Building_FakePlant2x2</thingClass>
		<description>An amber cereal plant that takes almost an entire growing season to come to fruition. This wheat has been subjected to drought conditions for too long and has wilted away.</description>
		<graphicData>
			<texPath>Things/Plant/DryWheat</texPath>
			<graphicClass>Graphic_Random</graphicClass>
			<drawSize>0.8</drawSize>
		</graphicData>
		<statBases>
			<MaxHitPoints>10</MaxHitPoints>
			<Flammability>2</Flammability>
		</statBases>
		<uiIconScale>0.5</uiIconScale>
		<altitudeLayer>Item</altitudeLayer>
		<rotatable>false</rotatable>
		<destroyable>true</destroyable>
		<useHitPoints>true</useHitPoints>
		<selectable>true</selectable>
		<pathCost>20</pathCost>
		<size>(1,1)</size>
		<building>
			<isEdifice>false</isEdifice>
			<claimable>false</claimable>
			<deconstructible>false</deconstructible>
		</building>
	</ThingDef>
</Defs>
