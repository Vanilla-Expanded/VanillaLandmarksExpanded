	<ThingDef ParentName="BuildingBase">
		<label>sinkhole</label>
		<defName>VEE_Sinkhole</defName>
		<description>A sudden collapse in the ground forming a deep, unstable pit, often exposing bare rock beneath the surface.</description>
		<thingClass>Crater</thingClass>
		<useHitPoints>false</useHitPoints>
		<rotatable>false</rotatable>
		<terrainAffordanceNeeded>Heavy</terrainAffordanceNeeded>
