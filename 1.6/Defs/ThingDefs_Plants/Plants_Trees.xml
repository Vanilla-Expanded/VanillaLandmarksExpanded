      </shadowData>
    </graphicData>
    <statBases>
      <Beauty>2</Beauty>
      <BeautyOutdoors>2</BeautyOutdoors>
    </statBases>
    <ingestible />
    <plant>
      <growDays>20</growDays>
      <harvestWork>800</harvestWork>
      <harvestYield>27</harvestYield>
    
      <leaflessGraphicPath>Things/Plant/TreeBirch_Leafless</leaflessGraphicPath>
