		<renderPrecedence>395</renderPrecedence>
		<texturePath>Terrain/Surfaces/VEE_SulfuricWaterDeepRamp</texturePath>
		<pollutedTexturePath>Terrain/Surfaces/VEE_SulfuricWaterDeepRamp</pollutedTexturePath>
		<toxicBuildupFactor>1</toxicBuildupFactor>
		<pathCost>400</pathCost>
		<traversedThought>VEE_SulfuricWater</traversedThought>
		<waterBodyType>Other</waterBodyType>
		<statBases>
			<Beauty>-4</Beauty>
		</statBases>
	</TerrainDef>
	<TerrainDef ParentName="WaterChestDeepBase">
		<defName>VEE_SulfuricWaterDeepMovingChestDeep</defName>
		<label>sulfuric chest-deep moving water</label>
		<renderPrecedence>399</renderPrecedence>
		<affordances>
			<li>MovingFluid</li>
		</affordances>
		<tags>
			<li>River</li>
		</tags>
		<waterDepthShaderParameters>
			<_UseWaterOffset>1</_UseWaterOffset>
		</waterDepthShaderParameters>
		<texturePath>Terrain/Surfaces/VEE_SulfuricWaterChestDeepRamp</texturePath>
		<pollutedTexturePath>Terrain/Surfaces/VEE_SulfuricWaterChestDeepRamp</pollutedTexturePath>
		<waterDepthShader>Map/WaterDepth</waterDepthShader>
		<toxicBuildupFactor>1</toxicBuildupFactor>
		<pathCost>150</pathCost>
		<traversedThought>VEE_SulfuricWater</traversedThought>
		<waterBodyType>Other</waterBodyType>
		<floodTerrain>ToxicFlood</floodTerrain>
		<statBases>
			<Beauty>-4</Beauty>
		</statBases>
	</TerrainDef>
	<TerrainDef ParentName="VEE_SulfuricWaterDeepShallowBase">
		<defName>VEE_SulfuricWaterFlood</defName>
		<label>shallow floodwater</label>
		<temporary>true</temporary>
		<tags>
			<li>Flood</li>
		</tags>
		<canFreeze>false</canFreeze>
		<!-- Thin ice and floodwater are both temp terrain, this means we can't support frozen floodwater -->
		<driesTo/>
		<!-- We don't want moisture pumps to turn temp terrain into stony soil -->
		<floodTerrain>VEE_SulfuricWaterFlood</floodTerrain>
		<renderPrecedence>325</renderPrecedence>
	</TerrainDef>
	<TerrainDef Name="VEE_SulfuricWaterDeepShallowBase" ParentName="WaterShallowBase">
		<defName>VEE_SulfuricWaterShallow</defName>
		<label>sulfuric shallow water</label>
		<renderPrecedence>394</renderPrecedence>
		<driesTo>Gravel</driesTo>
		<texturePath>Terrain/Surfaces/VEE_SulfuricWaterShallowRamp</texturePath>
		<pollutedTexturePath>Terrain/Surfaces/VEE_SulfuricWaterShallowRamp</pollutedTexturePath>
		<waterDepthShader>Map/WaterDepth</waterDepthShader>
		<toxicBuildupFactor>1</toxicBuildupFactor>
		<pathCost>100</pathCost>
		<traversedThought>VEE_SulfuricWater</traversedThought>
		<waterBodyType>Other</waterBodyType>
		<floodTerrain>VEE_SulfuricWaterFlood</floodTerrain>
		<statBases>
			<Beauty>-4</Beauty>
		</statBases>
	</TerrainDef>
