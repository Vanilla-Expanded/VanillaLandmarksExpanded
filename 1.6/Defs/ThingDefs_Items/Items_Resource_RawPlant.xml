<?xml version="1.0" encoding="utf-8"?>
<Defs>
	<ThingDef ParentName="PlantFoodRawBase">
		<defName>VEE_RawDates</defName>
		<label>dates</label>
		<description>Raw dates. Nice to eat, even when raw. They are sweet, nutrient-rich fruit harvested from date palms. They keep well in warm climates and provide a reliable source of energy.</description>
		<graphicData>
			<texPath>Things/Item/Dates</texPath>
		</graphicData>
		<statBases>
			<MarketValue>1.2</MarketValue>
			<Mass>0.027</Mass>
		</statBases>
		<comps>
			<li Class="CompProperties_Rottable">
				<daysToRotStart>30</daysToRotStart>
				<rotDestroys>true</rotDestroys>
			</li>
		</comps>
		<ingestible>
			<preferability>RawTasty</preferability>
			<foodType>VegetableOrFruit</foodType>
		</ingestible>
	</ThingDef>
	
</Defs>