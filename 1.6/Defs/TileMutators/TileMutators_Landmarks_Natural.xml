	<TileMutatorDef>
		<defName>VEE_BurnedForest</defName>
		<label>burned forest</label>
		<description>A forested area that has been recently affected by a severe wildfire.</description>
		<workerClass>VanillaExplorationExpanded.TileMutatorWorker_BurnedForest</workerClass>
		<biomeWhitelist>
			<li>BorealForest</li>
			<li>TemperateForest</li>
			<li>TropicalRainforest</li>
		</biomeWhitelist>
	</TileMutatorDef>
	<TileMutatorDef>
		<defName>VEE_FloodPlains</defName>
		<label>flood plains</label>
		<description>A low-lying stretch of land shaped by seasonal flooding, where nutrient-rich waters periodically spill over the banks and retreat, leaving fertile soil in their wake.</description>
		<categories>
			<li>River</li>
		</categories>
		<priority>2</priority>
		<overrideRiverbankTerrain>VEE_FertileRiverbank</overrideRiverbankTerrain>
		<workerClass>VanillaExplorationExpanded.TileMutatorWorker_LargeRiverBanks</workerClass>
		<maxHilliness>Flat</maxHilliness>
		<modExtensions>
			<li Class="VEF.Maps.TileMutatorExtension">
				<riverbankSizeMultiplier>4</riverbankSizeMultiplier>
			</li>
		</modExtensions>
	</TileMutatorDef>
	<TileMutatorDef>
		<defName>VEE_ContaminatedReservoir</defName>
		<label>contaminated reservoir</label>
		<description>A stagnant body of water saturated with lingering radiation, its depths unsafe for drinking or fishing and its shores hostile to life.</description>
		<categories>
			<li>Lake</li>
		</categories>
		<priority>2</priority>
		<workerClass>VanillaExplorationExpanded.TileMutatorWorker_ContaminatedReservoir</workerClass>
		<canSpawnOnRiver>false</canSpawnOnRiver>
		<modExtensions>
			<li Class="VEF.Maps.TileMutatorExtension">
				<thingToSpawn>VEE_RadioactiveBarrel</thingToSpawn>
				<thingToSpawnAmount>100~200</thingToSpawnAmount>
				<terrainValidation>
					<li>VEE_IrradiatedSoil</li>
				</terrainValidation>
			</li>
		</modExtensions>
	</TileMutatorDef>
