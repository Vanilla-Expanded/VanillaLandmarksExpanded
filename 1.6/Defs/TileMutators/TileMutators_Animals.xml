<?xml version="1.0" encoding="utf-8"?>
<Defs>
	<TileMutatorDef>
		<defName>VEE_AbundantPredators</defName>
		<label>abundant predators</label>
		<description>Animals considered predators appear in greater numbers here.</description>
		<workerClass>VanillaExplorationExpanded.TileMutatorWorker_Predators</workerClass>
		<chanceOnNonLandmarkTile>0.02</chanceOnNonLandmarkTile>
		<animalDensityRange>0.3~7</animalDensityRange>
		<pollutionRange>0~0.25</pollutionRange>
		<categories>
			<li>VEE_AnimalDensities</li>
		</categories>
	</TileMutatorDef>
	<TileMutatorDef>
		<defName>VEE_ReducedPredators</defName>
		<label>reduced predators</label>
		<description>Animals considered predators appear in lesser numbers here.</description>
		<workerClass>VanillaExplorationExpanded.TileMutatorWorker_ReducedPredators</workerClass>
