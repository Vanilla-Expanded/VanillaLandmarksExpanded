	<TileMutatorDef>
		<defName>VEE_MarineSanctuary</defName>
		<label>marine sanctuary</label>
		<description>Coastal animals appear in greater numbers here.</description>
		<workerClass>VanillaExplorationExpanded.TileMutatorWorker_MarineSanctuary</workerClass>
		<chanceOnNonLandmarkTile>0.02</chanceOnNonLandmarkTile>
		<coastSidesRange>1~5</coastSidesRange>
		<animalDensityRange>0.3~7</animalDensityRange>
		<pollutionRange>0~0.25</pollutionRange>
		<categories>
			<li>VEE_AnimalDensities</li>
		</categories>
	</TileMutatorDef>
	<TileMutatorDef MayRequire="VanillaExpanded.VanillaAnimalsExpanded">
		<defName>VEE_NocturnalFauna</defName>
		<label>nocturnal fauna</label>
		<description>Nocturnal and crepuscular animals appear in greater numbers here.</description>
		<workerClass>VanillaExplorationExpanded.TileMutatorWorker_Nocturnal</workerClass>
		<chanceOnNonLandmarkTile>0.02</chanceOnNonLandmarkTile>
		<biomeWhitelist>
			<li>BorealForest</li>
			<li>TemperateForest</li>
			<li>AridShrubland</li>
			<li>Desert</li>
			<li>ExtremeDesert</li>
			<li>TropicalRainforest</li>
			<li>TropicalSwamp</li>
		</biomeWhitelist>
		<animalDensityRange>0.3~7</animalDensityRange>
		<pollutionRange>0~0.25</pollutionRange>
		<categories>
			<li>VEE_AnimalDensities</li>
		</categories>
	</TileMutatorDef>
	<TileMutatorDef>
		<defName>VEE_RodentPlagues</defName>
		<label>rodent plagues</label>
		<description>This area is infested with rats.</description>
		<workerClass>VEF.Maps.TileMutatorWorker_ExtraAnimal</workerClass>
		<biomeBlacklist>
			<li>SeaIce</li>
		</biomeBlacklist>
		<chanceOnNonLandmarkTile>0.025</chanceOnNonLandmarkTile>
		<modExtensions>
			<li Class="VEF.Maps.TileMutatorExtension">
				<forcedPawnKindDefs>
					<li>
						<forcedPawnKindDef>Rat</forcedPawnKindDef>
						<forcedPawnKindDefChance>0.25</forcedPawnKindDefChance>
					</li>
				</forcedPawnKindDefs>
			</li>
		</modExtensions>
	</TileMutatorDef>
